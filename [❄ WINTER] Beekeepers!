--Game Gui
if game.PlaceId == 7905476552 then 
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = OrionLib:MakeWindow({Name = "[‚ùÑ WINTER] Beekeepers!", HidePremium = false, IntroEnable = false,IntroText = "Made by Scropt", SaveConfig = true, ConfigFolder = "OrionTest"})


--FarmTab
local FarmTab = Window:MakeTab({
      Name = "Auto Farme‚öôÔ∏è",
      Icon = "rbxassetid://4483345998",
      PremiumOnly = false
})
--Auto Convert 
_G.AutoConvert = true
--Fonction
   function AutoConvert()
      while _G.AutoConvert == true do
        game:GetService("ReplicatedStorage").Shared.Network.SellFood:FireServer()
         wait(.01)
   end
   end
--Toggle
FarmTab:AddToggle({
      Name = "Auto Convert",
      Default = false,
      Callback = function(Value)
      _G.AutoConvert = Value
      AutoConvert()
      end    
})


--Teleport Tab
local TpTab = Window:MakeTab({
      Name = "Place - Tpüåå",
      Icon = "rbxassetid://4483345998",
      PremiumOnly = false
})
--Teleport
getgenv().TPPlace = "string"
--Liste Field Tp
TpTab:AddDropdown({
      Name = "Select A field",
      Default = "1",
      Options = {"Cherry Field", "Orange Field", "Bamboo Field", "Cactus Field", "Pumpking Field"},
      Callback = function(Value)
      getgenv().TPPlace = Value
      end    
})
--Choose Field TP
TpTab:AddButton({
      Name = "Teleport",
      Callback = function()
      if getgenv().TPPlace == "Cherry Field" then
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
      elseif getgenv().TPPlace == "Orange Field" then
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
      elseif getgenv().TPPlace == "Bamboo Field" then
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
      elseif getgenv().TPPlace == "Cactus Field" then
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()
      elseif getgenv().TPPlace == "Pumpking Field" then
      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new()

      end
   end
})
--Liste hive
TpTab:AddDropdown({
      Name = "Select A Hive",
      Default = ")",
      Options = {"1", "2", "3", "4", "5", "6"},
      Callback = function(Value)
      getgenv().TPPlace = Value
      end    
})
--Choose hive
TpTab:AddButton({
      Name = "Teleport",
      Callback = function()
      if getgenv().TPPlace == "1" then
